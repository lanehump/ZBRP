## You CAN edit the following:
endpoint_add_tcp "0.0.0.0:30120"
endpoint_add_udp "0.0.0.0:30120"
sv_maxclients 48
sv_enforceGameBuild 2944 #mp2023_01 - San Andreas Mercenaries
set steam_webApiKey "none"
sets tags "default, deployer"

set mysql_connection_string "mysql://root@localhost:3306/Server"

## You MAY edit the following:
sv_licenseKey "cfxk_JluEA2s3QXK19DqOrH3r_3qbsK0"
sv_hostname "lane built with CFX Default FiveM!"
sets sv_projectName "[CFX Default FiveM] lane"
sets sv_projectDesc "Recipe for the base resources required to run a minimal FiveM server."
sets locale "root-AQ" 
sets sv_pureLevel 1
 sv_enforcegamebuild 2802
## These resources will start by default.
ensure [managers]
ensure chat
ensure fivem-map-hipster
ensure hardcap
ensure vMenu
ensure npcbegone
ensure recoil
ensure metafiles
ensure erp_progressbar
ensure ffp
ensure emotes
ensure SP45
ensure MK18
#ensure Inventory



## Add system admins
add_ace group.admin command allow # allow all commands
add_ace group.admin command.quit deny # but don't allow quit
add_principal identifier.fivem:2935627 group.admin #l4ne1
